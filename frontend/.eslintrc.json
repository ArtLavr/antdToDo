{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
        // "only-warn"
    ],
    "rules": {
        "react/prop-types": "warn",
        "no-unused-vars" : ["warn",{ "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "semi": ["warn", "never"],
        "quotes": ["warn", "double"]
    }
}
